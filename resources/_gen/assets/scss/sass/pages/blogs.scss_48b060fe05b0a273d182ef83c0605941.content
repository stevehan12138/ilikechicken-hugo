.title{font-size:60px;background:linear-gradient(to right,#F7C7DB 0%,#F68ECE 100%);background:#f7c7db;background-clip:text;-webkit-background-clip:text;-webkit-text-fill-color:transparent}.list{margin:20px -10px;margin-top:50px}.list-item{margin-bottom:40px}.list-item h2>a{font-size:30px;color:#f7c7db}.list-item h2>a:hover{color:#f68ece}.list-item.other-posts{font-size:18px}.list-item.other-posts h2>a{font-size:25px}.page-content{font-size:18px;min-height:600px}pre{padding:10px;border:3px solid #f7c7db}