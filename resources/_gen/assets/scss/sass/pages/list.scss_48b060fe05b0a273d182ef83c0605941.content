.back-link{color:#54b3d6;padding:0 .25rem;padding-top:.5rem;transition:color .3s ease-in-out,box-shadow .3s ease-in-out;text-decoration:none;font-size:20px;margin-bottom:20px}.back-link:hover{box-shadow:inset 100px 0 #54b3d6;color:#fff}.title{font-size:60px;background:linear-gradient(to right,#F7C7DB 0%,#F68ECE 100%);background:#f7c7db;background-clip:text;-webkit-background-clip:text;-webkit-text-fill-color:transparent}.list{margin:20px -10px;margin-top:50px}.list-item{margin:40px 0}.list-item h2>a{font-size:30px;color:#f7c7db}.list-item h2>a:hover{color:#f68ece}